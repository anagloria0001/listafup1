// inclua as bibliotecas e definas as variáveis globais de entrada e saída do modelo
#include <stdlib.h>
#include <stdio.h>


                      
int a = 4, b = 2, c = 5, d = 8;

// -- escreva seu código abaixo, não altere esta linha



int main() {
    if(b<a){int temp=b;b=a;a=temp;}
    if(c<a){int temp=c;c=a;a=temp;}
    if(d<a){int temp=d;d=a;a=temp;}
    if(c<b){int temp=c;c=b;b=temp;}
    if(d<b){int temp=d;d=b;b=temp;}
    if(d<c){int temp=d;d=c;c=temp;}
    
    printf(" %d\n %d\n %d\n %d\n", a, b, c, d);

}
